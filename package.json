{
  "private": true,
  "scripts": {
    "acquire": "node ./dist/acquire.js > ./data/data.json",
    "contributorAdd": "all-contributors add",
    "contributorGenerate": "all-contributors generate",
    "docs": "documentation build src -f md -o docs.md",
    "generate": "node ./dist/run.js",
    "generate-es5": "npm run generate && cross-env BABEL_ENV=publish babel output --out-dir output",
    "markdown": "remark *.md --quiet",
    "pretest": "eslint src",
    "prepublish": "del-cli dist && cross-env BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/",
    "report": "nyc report --reporter=html",
    "test": "cross-env BABEL_ENV=test BABEL_DISABLE_CACHE=1 nyc --check-coverage --lines 100 ava",
    "test-012": "cross-env BABEL_ENV=test BABEL_DISABLE_CACHE=1 nyc --check-coverage --lines 100 ava"
  },
  "keywords": [
    "css"
  ],
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "babel-generator": "^6.7.2",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-template": "^6.7.0",
    "babel-traverse": "^6.7.3",
    "babel-types": "^6.7.2",
    "camelcase": "^2.1.1",
    "chalk": "^1.1.1",
    "css-color-names": "0.0.4",
    "ends-with": "^0.2.0",
    "flatten": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "html-entities": "^1.2.0",
    "map-stream": "0.0.6",
    "nano-equal": "^1.0.1",
    "object-assign": "^4.0.1",
    "postcss": "^5.0.19",
    "rollup": "^0.34.7",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-vinyl": "^0.2.0",
    "vinyl": "^1.2.0",
    "vinyl-fs": "^2.4.3"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.6",
    "ava": "^0.15.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-precompile-charcodes": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.6",
    "cross-env": "^1.0.7",
    "del-cli": "^0.2.0",
    "documentation": "^4.0.0-beta9",
    "eslint": "^3.0.0",
    "eslint-config-cssnano": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.12.0",
    "execa": "^0.4.0",
    "got": "^6.2.0",
    "lerna": "2.0.0-beta.24",
    "listr": "^0.5.0",
    "mdast-util-heading-range": "^2.0.1",
    "nyc": "^7.1.0-candidate2",
    "postcss-value-parser": "^3.3.0",
    "remark-cli": "^1.0.0",
    "remark-heading-gap": "^2.0.1",
    "stylelint": "^7.1.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "packages/css-values/index.js"
    ],
    "exclude": [
      "packages/**/test.js",
      "**/__tests__"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "ava": {
    "files": [
      "packages/**/test.js",
      "src/__tests__/**/*.js"
    ],
    "babel": "inherit",
    "require": "babel-register"
  },
  "eslintConfig": {
    "extends": "cssnano"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "precompile-charcodes"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "remarkConfig": {
    "output": true,
    "plugins": {
      "dist/docs/completed.js": {},
      "heading-gap": {}
    },
    "settings": {
      "bullet": "*",
      "fences": true,
      "listItemIndent": "1"
    }
  }
}
